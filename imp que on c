  #include<stdio.h>
      int S(int n){
      int sum=0;
      for(int i=1;i<=n;++i){
			 
     	if(i%2 == 1){
     	sum = sum + i;
     				
	 }
	 else
	 sum = sum - i;
				 
            }
             }
	
	 return sum;
        }
     	  main(){
          int n;
     	  scanf("%d",&n);
     	  printf("%d",S(n));
			  
          }
