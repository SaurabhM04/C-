 //program to find out the greatest no among three no using ternary operator.
 
    int main(){
    
    int a , b ,c , great;
    printf("Enter three number: \n");
    scanf("%d %d %d" , &a, &b, &c);
    great = a>b? (a>c? a:c) : (b>c? b:c);
    printf("the greatest number is %d \n", great);
    return 0;
}
   
   
