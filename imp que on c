	 //pattern
	 #include<stdio.h>
	  #include <stdlib.h>
	   star(int n,int h){
	  	for(int line=1; line<h; line++){
	  		for(int space=0; space<line; space++){
	  			printf(" ");
			  }
			  printf("*\n");
		  }
		  for(int line=0; line<h-1;line++){
		  	for(int space=0;space<h-line-2;space++){
		  		printf(" ");
			  }
			  printf("*\n");
		  }
	  }
	  int main(){
	  	int n;scanf("%d",&n);
	  	if(n>0){
	  		printf("*\n");
	  		for(int i=1;i<=n;i++){
	  			star(n,4);
			  }
		  }
	  }
 

 
