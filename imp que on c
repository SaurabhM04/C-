	int too=0;
	int rs(int x,int y){
		int r;
		do{
			r=y%x;
			y=x;
			x=r;
			too++;
		}while(r!=0);
		return y;
	}	
	int main(){
		printf("%d,%d",rs(24,10),too);
		return 0;
	}
	
