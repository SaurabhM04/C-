//Bell NUMBER
    #include<stdio.h>
        main(){
		
		int bell[10][10],line=5,i,j;
		bell[0][0]=1;
		for(i=1;i<line;i++){
			for(j=0;j<=i;j++){
				if(j==0){
					bell[i][j]=bell[i-1][i-1];
				}
				else{
					bell[i][j]=bell[i][j-1]+bell[i-1][j-1];
				}
			}
		}
		 for(i=0;i<=line;i++){
		 	for(j=0;j<=i;j++){
		 		printf("%d\t",bell[i][j]);
			 }
			 printf("\n");
		 }
		 return 0;
	}
	
