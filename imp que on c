#include<stdio.h>
 int armstrong (int x);
 int main(){
    int x;
    printf("number:");
    scanf("%d,&x");
    if (x == armstrong(x)){
    	printf("number is armstrong\n");
	}
}
    int armstrong(int x){
    	int s = 0,q,r;
     	do{
		q=x/10;
		r=x%10;
		s=s+r*r*r;
		x=q;
	} while(q!=0);
     	return s;
     }
     
