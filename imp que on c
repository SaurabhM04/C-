    	int main(){
    		int n, sum = 0;
    		printf("enter the number n:\n");
    		scanf("%d", &n);
    		for(int i=1; i<=n; i++){
    			if(n<0)
    			{
    				break;
				}
				sum = sum + n;
			}
			printf("sum %d\n", sum);
			return 0;
		}	
		
               //break
		int main(){
			char operater;
			int  s ,m;
			printf("enter (s) (+, -,*, / ) (m):\n");
			scanf("%d %c %d", &s, &operater, &m);
		switch(operater)
			{
				case'+':
					result= s + m;
					break;
				case '-':
			    	result= s - m;
			    	break;
			    case '*':
			    	result= s * m;
			    	break;
			    case '/':
			    	result= s / m;
			    	break;
			    default:
			    	printf("invalid operater");
			}
			printf("%d %c %d = %d",s, operater, m, result);
			return 0;
		}
		
